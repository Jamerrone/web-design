{
  "name": "project-boilerplate",
  "description": "",
  "version": "1.0.0",
  "license": "MIT",
  "keywords": [],
  "scripts": {
    "test": "eslint src",
    "clean": "rimraf dist",
    "copy:assets": "cpx 'src/assets/**/*' 'dist/assets'",
    "copy:html": "cpx 'src/**/*.html' dist",
    "copy": "run-p copy:** --silent",
    "bundle": "rollup -c --environment production",
    "watch:assets": "cpx 'src/assets/**/*' 'dist/assets' -w",
    "watch:bundle": "rollup -c -w",
    "watch:html": "cpx 'src/**/*.html' dist -w",
    "start": "npm-run-all clean -p watch:** --silent",
    "build": "run-s clean test copy bundle --silent"
  },
  "author": "James Peter Perrone Jefferies <james.p.perrone@gmail.com>",
  "repository": "https://github.com/Jamerrone/project-boilerplate.git",
  "bugs": "https://github.com/Jamerrone/project-boilerplate/issues",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.6.1",
    "cpx": "^1.5.0",
    "cssnano": "^3.10.0",
    "eslint": "^4.19.1",
    "eslint-config-google": "^0.9.1",
    "hyperscript": "^2.0.2",
    "hyperscript-helpers": "^3.0.3",
    "npm-run-all": "^4.1.2",
    "postcss-cssnext": "^3.1.0",
    "precss": "^3.1.2",
    "rimraf": "^2.6.2",
    "rollup": "^0.57.1",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-commonjs": "^9.1.0",
    "rollup-plugin-filesize": "^1.5.0",
    "rollup-plugin-livereload": "^0.6.0",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-postcss": "^1.5.1",
    "rollup-plugin-progress": "^0.4.0",
    "rollup-plugin-serve": "^0.4.2",
    "rollup-plugin-uglify": "^3.0.0"
  }
}
